<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C6C0C0"
    tools:context=".ui.piloto.ProjetoActivity">

    <TextView
        android:id="@+id/_project"
        android:layout_width="400dp"
        android:layout_height="41dp"
        android:background="#CFD3DA"
        android:fontFamily="sans-serif-medium"
        android:shadowColor="#201B1B"
        android:text="Projetos"
        android:textColor="#007be5"
        android:textSize="32dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="168dp" />

    <ImageView
        android:id="@+id/imgstar1"
        android:layout_width="118dp"
        android:layout_height="116dp"
        app:srcCompat="@drawable/piloto"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="15dp" />

    <ImageView
        android:id="@+id/imgstar2"
        android:layout_width="37dp"
        android:layout_height="31dp"
        app:srcCompat="@drawable/_star"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="72dp" />

    <ImageView
        android:id="@+id/imgstar3"
        android:layout_width="37dp"
        android:layout_height="31dp"
        app:srcCompat="@drawable/_star"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="224dp"
        tools:layout_editor_absoluteY="72dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="37dp"
        android:layout_height="31dp"
        app:srcCompat="@drawable/_star"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="72dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/baseline_menu_24"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="38dp" />

    <TextView
        android:id="@+id/txt_piloto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="Piloto"
        android:textColor="#010819"
        android:textSize="25dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="38dp" />

    <TextView
        android:id="@+id/txtevento"
        android:layout_width="120dp"
        android:layout_height="33dp"
        android:background="#cfd3da"
        android:fontFamily="sans-serif-medium"
        android:text="Evento"
        android:textColor="#331E5E"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="358dp" />

    <ImageView
        android:id="@+id/img_drone"
        android:layout_width="99dp"
        android:layout_height="88dp"
        android:background="#FAF4F4"
        android:backgroundTint="#FBF7F7"
        app:srcCompat="@drawable/drone"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="254dp" />

    <TextView
        android:id="@+id/txtlocale"
        android:layout_width="306dp"
        android:layout_height="35dp"
        android:background="#F3EFEF"
        android:fontFamily="sans-serif-medium"
        android:text="Local:"
        android:textSize="18sp"
        android:visibility="visible"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="416dp"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/txtdate"
        android:layout_width="306dp"
        android:layout_height="35dp"
        android:background="#F8F5F5"
        android:fontFamily="sans-serif-medium"
        android:text="Data:     /    /"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="473dp" />

    <TextView
        android:id="@+id/txtvalue"
        android:layout_width="306dp"
        android:layout_height="35dp"
        android:background="#FAF6F6"
        android:fontFamily="sans-serif-medium"
        android:text="R$ 0000.00"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="530dp" />

    <Button
        android:id="@+id/btn01"
        android:layout_width="101dp"
        android:layout_height="40dp"
        android:backgroundTint="#6792C7"
        android:fontFamily="sans-serif-black"
        android:text="Aceitar"
        android:textColor="#06062C"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="618dp" />

    <Button
        android:id="@+id/btn02"
        android:layout_width="101dp"
        android:layout_height="40dp"
        android:backgroundTint="#6792C7"
        android:fontFamily="sans-serif-black"
        android:text="Recusar"
        android:textColor="#06062C"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="155dp"
        tools:layout_editor_absoluteY="618dp" />

    <Button
        android:id="@+id/btn03"
        android:layout_width="101dp"
        android:layout_height="40dp"
        android:backgroundTint="#6792C7"
        android:fontFamily="sans-serif-black"
        android:text="Proposta"
        android:textColor="#06062C"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="618dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
